snippet if "if" b
if($1) {
	$2
}
endsnippet

snippet for "for" b
for(let ${1:i} = 0; $1 < ${2:array.length}; $1++) {
	$3
}
endsnippet

snippet fore "for of" b
for(let $1 of $2) {
	$3
}
endsnippet

snippet cl "console.log" b
console.log($1)
endsnippet

snippet f "function" b
function ${1:f}($2) {
	$3
}
endsnippet

snippet => "arrow function" w
($1) => $2
endsnippet

snippet [][] "2D array" b
let $1 = Array($2).fill(null).map(() => Array($3).fill($4))
endsnippet
